<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".Main.MenuPrincipal">


        <FrameLayout
            android:id="@+id/menuPrincipalFrame"
            android:layout_above="@id/bottom_navigationMenuPrincipal"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <com.aurelhubert.ahbottomnavigation.AHBottomNavigation
            android:layout_alignParentBottom="true"
            android:background="@color/couleur_blanche"
            android:id="@+id/bottom_navigationMenuPrincipal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="bottom" />


    </RelativeLayout>




</LinearLayout>

